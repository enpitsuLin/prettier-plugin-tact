{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "lib": [
      "es2021"
    ],
    "rootDir": "src",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "declaration": true,
    "outDir": "out",
    "sourceMap": true,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": [
    "node_modules",
    "lib"
  ]
}
